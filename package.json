  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.9.1",
    "parcel-bundler": "^1.9.3",
    "truffle": "^4.1.14",
    "truffle-hdwallet-provider": "0.0.6"
  },
  "scripts": {
    "start": "npm run start:aragon:ipfs",
    "start:aragon:ipfs": "aragon run --app-init-args 0xe743212601fFDF381d6cD68af1C4D6E8355e205D 1000000000",
    "start:aragon:http": "aragon run --http localhost:8001 --http-served-from ./dist --app-init-args 0xe743212601fFDF381d6cD68af1C4D6E8355e205D 1000000000",
    "start:app": "npm run sync-assets && npm run build:script -- --no-minify && parcel serve app/index.html -p 8001 --out-dir dist/ --no-cache",
    "test": "aragon contracts test",
    "compile": "aragon contracts compile",
    "sync-assets": "copy-aragon-ui-assets -n aragon-ui ./dist",
    "build:app": "parcel build app/index.html -d dist/ --public-url '.' --no-cache",
    "build:script": "parcel build app/script.js -d dist/ --no-cache",
    "build": "npm run sync-assets && npm run build:app && npm run build:script",
    "publish": "aragon apm publish",
    "deploy:rpc": "truffle compile && truffle exec scripts/deploy.js --network rpc",
    "deploy:rinkeby": "truffle compile && truffle exec scripts/deploy-althea-dao-factory.js --network rinkeby",
    "deploy:devnet": "truffle exec scripts/deploy-althea-dao-factory.js --network devnet",
    "publish:devnet": "npm run compile && aragon publish --apm.ipfs.rpc.protocol http --apm.ipfs.rpc.host ipfs.aragon.network --no-confirm=true --keyfile ../../.localkey.json \"$(npm run deploy:devnet | tail -n 1)\"",
    "publish:rpc": "npm run compile && aragon publish --no-confirm=true \"$(npm run deploy:rpc | tail -n 1)\"",
    "publish:rinkeby": "npm run compile && aragon publish --no-confirm=true --keyfile ~/.rinkebykey.json \"$(npm run deploy:rinkeby | tail -n 1)\""
  },
  "keywords": [],
  "license": "GPL-3.0"
